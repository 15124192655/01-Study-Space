  file = {E\:\\文献库\\Zotero\\storage\\BBCFII8F\\Hou et al-2023-Meta-Gating Framework for Fast and Continuous Resource Optimization in Dynamic.pdf;E\:\\文献库\\Zotero\\storage\\H23HGLRV\\10172335.html}
}

@thesis{HuAiHuaJiYuZaiHouFuZaHuanJingDeWuXianDingWeiGuanJianJiShuYanJiu2022,
  type = {博士},
  title = {基于灾后复杂环境的无线定位关键技术研究},
  author = {{胡爱华}},
  date = {2022},
  institution = {{北京邮电大学}},
  doi = {10.26969/d.cnki.gbydu.2022.000300},
  url = {https://kns.cnki.net/kcms2/article/abstract?v=3uoqIhG8C447WN1SO36whFuPQ0yKi4pXSQlJ_W8wBD9JRPlAs_d8B9ldksi7u3VZP_WTW_RKPQ_8zehXtNZdUGLnicGG3sY-&uniplatform=NZKPT},
  urldate = {2023-10-13},
  abstract = {随着移动通信和物联网技术的飞速发展,位置感知不仅成为了研究热点,也成为保障人民生命财产安全必不可少的一部分。在应急救援、抗震救灾等任务中,精准位置测量是保障救援工作顺利开展的重要支撑。传统无线通信定位参数估计方法,很难适应灾后现场的复杂无线传播环境。开展智能、联合、高效的复杂环境参数估计方法研究具有重要意义。本文针对通信环境的复杂性带来的定位系统几何构型受限制、定位参数测量误差大等问题,提出了融合多元测距参数的联合定位系列算法,减小了无线信号测距误差,论文主要工作与创新如下:1.针对地震灾后建筑物倒塌随机性大,电磁信号传播环境复杂,信道模型的环境依赖难题,对三种典型倒塌环境的无线信道进行了研究。提出了联合锐化归一功率时延谱的灾后环境无线信道建模方法,利用该方法建立了适用于灾后典型环境的路径损耗模型、均方根时延扩展模型以及抽头延迟线信道模型。本文所构建信道模型的时延均值与对比模型相比,三种典型环境分别提高了 28.92\%、16.39\%、20.63\%。2.针对灾后废墟环境信号杂散严重,各向传播特性不同,定位系统几何构型对定位精度影响大的问题,提出了一种面向多变环境下的各向异性定位几何构型优化方法。该方法构建了联合定位的目标函数,提出了双向搜索交叉优化算法,获得了目标函数的实时最优解,并转化为灾后环境中移动基站的最优布站方案。该方法与其他优化方法相比,有效排除了局部最优,具有更好的全局优化性能,优化后定位几何精度提高了 16.18\%。3.针对灾后环境无线信号传播受阻、信号衰减和非视距传播严重,探测设备捕获信号的距离和方向受限的问题,提出一种基于半定规划的灾后无线定位探测设备优选方法。该方法通过构建多探测设备的定位模型,生成带有可变节点的决策向量,并由定位目标的快速粗定位确定参考节点和惩罚函数,基于半定规划的节点选择算法寻找最优的节点组合,建立双级探测设备优选策略,完成精确定位。该算法性能优于其他参考节点已知优化算法,定位节点正确选择准确率高于90\%。相对于固定主节点的优化算法定位精度提高了 15.8\%。4.针对灾后环境无线信号多径传播严重,无法准确识别首达径,无线测量精度差的问题,提出了一种面向灾后环境的基于先验环境数据的深度学习定位信号测量方法,该方法对定位中的信道频域响应进行训练,通过深度学习智能提取首达径时间信息和到达角度。经过线下训练和数据的验证,首达径的到达时间和到达角的估计准确率能达到87\%,本文提出的方法与较优的旋转因子不变法相比到达时间的估计精度提高了 29.63\%,到达角度的估计精度提高了 38.7\%。},
  langid = {chinese},
  keywords = {/unread,channel transfer function,optimization,power delay spectrum,Wireless location,优化,信道传输函数,功率时延谱,无线定位},
  file = {E:\文献库\Zotero\storage\9XMHCYNV\胡_2022_基于灾后复杂环境的无线定位关键技术研究.pdf}
}

