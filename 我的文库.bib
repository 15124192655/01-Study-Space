@thesis{zhaoshiqiangJiYuRSSIDeWSNDingWeiSuanFaJiCeJuYingXiangYinSuDeYanJiu2014,
  type = {硕士},
  title = {{{基于RSSI的WSN定位算法及测距影响因素的研究}}},
  author = {赵士强 and 钱志鸿},
  date = {2014},
  institution = {{吉林大学}},
  abstract = {随着物联网概念的普及和研究的深入，作为其核心技术之一的无线传感器网络技术更加引起各国研究人员的关注热情，在军事、医疗、建筑和交通等领域的应用层出不穷，这些应用的共同特点是都需要获取事件发生的位置信息，这就涉及到无线传感器网络定位技术。一般来说，WSN的定位可以分为两种类型：基于距离的和与距离无关的。与距离无关的定位方法不需要测距，这降低了硬件成本，但它的定位精度比基于距离的定位方法要差一些。基于RSSI测距的定位方法的优势在于其便捷和低成本的特性，如果能采取一定的措施来降低测距和定位算法带来的误差，这种定位方法将具有更加广阔的商业应用前景。},
  keywords = {无线传感器网络;RSSI测距;常态分布;距离修正;加权质心,论文结构},
  file = {E:\文献库\Zotero\storage\UUIKVFLD\赵士强_钱志鸿_2014_基于RSSI的WSN定位算法及测距影响因素的研究.pdf}
}

@article{ZhaoYanJiYuChaiFenMoXingDeJinChangWuYuanDingWeiSuanFa2023,
  title = {基于差分模型的近场无源定位算法},
  author = {{赵研} and {陶海红} and {畅鑫} and {董春曦} and {赵国庆}},
  date = {2023},
  journaltitle = {雷达科学与技术},
  volume = {21},
  number = {4},
  pages = {405-410+419},
  issn = {1672-2337},
  url = {https://kns.cnki.net/kcms2/article/abstract?v=3uoqIhG8C44YLTlOAiTRKu87-SJxoEJu6LL9TJzd50nJJdZ1z2O24PJ1Zjy3Rjm7B5spH8QMsBIkxMd3wVF_bJEi4MwjGUHO&uniplatform=NZKPT},
  urldate = {2023-10-11},
  abstract = {对于均匀线性天线阵列近场目标定位研究是探测领域重要难点之一。本文根据前期近场基于差分迭代传播计算模型研究基础，提出一种新型高精度近场无源定位算法，针对均匀线性阵列近场目标空间几何结构，突破传统菲涅尔近似近场传播模型的束缚，建立全新高精度目标关系模型并形成定位解析表达式，通过阵列处理估计改进到达时差（TOA）定位观测量，利用新算法快速准确地获取近场多个独立同分布目标的定位信息。仿真实验结果表明：新算法机理不同于传统近场估计分析，特别是定位解析式的引入使得位置精度提升的同时计算量大为降低，更好地适应了近场非线性关系结构，性能分析显示TOA误差对定位估计精度的影响较为有限，在典型信噪比条件下新算法的距离和角度估计精度优于几种常规处理算法，未来具有深入研究的价值。},
  langid = {chinese},
  keywords = {/unread,difference model,Fresnel approximation,near-field,passive location,time difference of arrival(TDOA),到达时间差,差分模型,无源定位,菲涅尔近似,近场},
  annotation = {CSSCI: 北大核心},
  file = {E:\文献库\Zotero\storage\RF33J9JK\赵 et al_2023_基于差分模型的近场无源定位算法.pdf}
}

@thesis{ZhaoZhengYeJinChangXinYuanDingWeiSuanFaYanJiu2022,
  type = {硕士},
  title = {近场信源定位算法研究},
  author = {{赵政晔}},
  date = {2022},
  institution = {{西安电子科技大学}},
  doi = {10.27389/d.cnki.gxadu.2022.000820},
  url = {https://kns.cnki.net/kcms2/article/abstract?v=3uoqIhG8C475KOm_zrgu4lQARvep2SAkdnYma8rofBkEatduVB8YevY98PaW_3qv4faH-0_KsGSchzOBLqlry04ixxY3wIG5&uniplatform=NZKPT},
  urldate = {2023-10-11},
  abstract = {信源定位技术广泛应用于雷达、地震学、声纳、电子监视等领域,随着信源定位技术的发展,近场信源定位算法研究成为信号处理领域的研究热点之一。本文针对近场信源定位算法存在的估计精度不高、算法复杂度过大等问题,在分析已有算法的基础上,主要在以下三个方面做出了改进:1.当接收阵列为均匀线阵时,改进了传统的近场多重信号分类(Multiple Signal Classification,MUSIC)算法。算法的改进主要体现在算法复杂度上。利用高阶累积量的灵活性将信源的角度和距离分开,从而将二维谱峰搜索分解为搜索步长较大的一维谱峰搜索,以及搜索步长较小的小范围二维谱峰搜索。经过仿真实验,该算法与传统MUSIC算法相比,在估计精度要求相同的条件下可以显著的降低算法的复杂度。2.当接收阵列为均匀线阵时,研究了基于二阶矩的ESPRIT近场源定位算法,并在此基础上在算法步骤以及配对算法上进行了改进。首先在求得的协方差矩阵的基础上,通过选取反对角线以及与反对角线平行的向上或向下m个单位的反对角线元素得到虚拟阵元接收信号自相关,再将其以堆叠的方式构造出Toeplitz矩阵,其形式类似于远场的协方差矩阵。通过ESPRIT算法,求出对应的参数,并通过简单的配对可以成功定位信源。经过仿真实验,该算法与SALONS算法相比运算复杂度有所降低,且估计性能相当。3.当接收阵列为十字阵列时,研究了三维近场源定位算法。首先将基于二阶矩的ESPRIT近场源定位算法扩展到三维空间,十字阵列充分利用了x轴与y轴的正交性,因此可以分别根据其对应协方差矩阵进行估计,分别得到x轴和y轴的参数,此时再根据距离关系进行配对,从而实现信源的三维定位。同时研究了基于累积量的三维近场源定位算法,通过构造不同的累积量矩阵体现旋转因子,利用子空间的相似关系,求解旋转矩阵,经过简单的配对算法可以求出配对的俯仰角、方位角和距离。实验证明,本算法能够有效利用阵元,并正确的估计出信号的俯仰角、方位角和距离等三维参数。},
  langid = {chinese},
  keywords = {/unread,ESPRIT,Higher order cumulants,matching algorithm,MUSIC,Near field source localization,SALONS algorithm,SALONS算法,近场源定位,配对算法,高阶累积量},
