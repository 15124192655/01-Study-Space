@article{zhangxinrongYiZhongJiYuRSSIDeWuXianChuanGanQiWangLuoQuanChengYouHuaFenBuShiDingWeiCeLue2016,
  title = {{{一种基于RSSI的无线传感器网络全程优化分布式定位策略}}},
  author = {{张新荣} and {熊伟丽} and {徐保国}},
  date = {2016},
  journaltitle = {传感技术学报},
  shortjournal = {传感技术学报},
  volume = {29},
  number = {12},
  pages = {1875--1881},
  abstract = {针对目前定位算法存在精度与成本有较大矛盾的问题,在定位实施各阶段采取有效措施,提出一种新的基于接收信号强度指示(RSSI)观测模型的协作定位算法。该算法以有效计算方式获得距离数据,优选锚节点参与定位计算,通过泰勒展开迭代求精法实施位置优化,最终获得满足要求且定位精度较高的位置估计。引入协作定位思想,将满足一定要求的已定位节点升级为锚节点,参与其他节点的定位,提高了定位的覆盖率和定位精度。仿真实验结果显示:网络参数相同的条件下,本文算法定位效果接近基于实际坐标的泰勒级数展开算法,而远高于基本的最小二乘定位算法,且由于所需存储空间小,其定位精度也随测距误差的减小而快速提高,能够满足大规模无线传感器网络的定位需求。},
  isbn = {1004-1699},
  keywords = {无线传感器网络;RSSI测距;优化算法;协作定位},
  annotation = {00000  CSCD: 核心库 中文核心期刊/北大核心: 是 南农高质量: 三类},
