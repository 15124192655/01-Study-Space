  langid = {chinese},
  keywords = {⭐⭐},
  annotation = {sciif: 0.91 sci: Q4},
  file = {E:\文献库\Zotero\storage\IBGZGZJP\Wang et al_2020_Magnetic field propagation model of low frequency-very low communication based.pdf}
}

