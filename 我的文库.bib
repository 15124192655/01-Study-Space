@inproceedings{zhangNovelMapMatching2022,
  title = {A Novel Map Matching Based Localization Method for {{ISAC}}},
  booktitle = {2022 {{IEEE}}/{{CIC International Conference}} on {{Communications}} in {{China}} ({{ICCC Workshops}})},
  author = {Zhang, Zili and Huang, Jie and Wang, Cheng-Xiang and Sun, Jian and Zhang, Wensheng and He, Zhuoran},
  date = {2022-08},
  pages = {342--347},
  issn = {2474-9133},
  doi = {10.1109/ICCCWorkshops55477.2022.9896650},
  url = {https://ieeexplore.ieee.org/abstract/document/9896650},
  urldate = {2023-10-12},
  abstract = {In this paper, a novel map matching based localization method for integrated sensing and communication (ISAC) between two devices is proposed. There are two main stages for the localization method. Firstly, each of the two ISAC devices in the same environment senses the surroundings to obtain a position map of the environmental scatterers. Because the two devices are in the same environment, there will be some common scatterers in both the two maps. In the second stage, a device receives the map sent from the other through the communication function and makes the common scatterers in its map coincide with that in the other map. Because the positions of the common scatterers relative to the two devices have all been obtained by the previous sensing stage, through the geometric relationship between the positions of the common scatterers and that of the two devices, the relative position information between the two devices can be obtained. The localization performance of the proposed method is verified through extensive simulations.},
  eventtitle = {2022 {{IEEE}}/{{CIC International Conference}} on {{Communications}} in {{China}} ({{ICCC Workshops}})},
  keywords = {/read,/unread},
