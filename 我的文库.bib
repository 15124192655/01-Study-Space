@online{abbasiTHzBandChannel2021,
  title = {{{THz Band Channel Measurements}} and {{Statistical Modeling}} for {{Urban Microcellular Environments}}},
  author = {Abbasi, Naveed A. and Gomez-Ponce, Jorge and Kondaveti, Revanth and Kumar, Ashish and Bhagat, Eshan and Rao, Rakesh N. S. and Abu-Surra, Shadi and Xu, Gary and Zhang, Charlie and Molisch, Andreas F.},
  date = {2021-12-03},
  eprint = {2112.01770},
  eprinttype = {arxiv},
  eprintclass = {eess},
  doi = {10.48550/arXiv.2112.01770},
  url = {http://arxiv.org/abs/2112.01770},
  urldate = {2022-10-17},
  abstract = {The THz band (0.1-10 THz) has attracted considerable attention for next-generation wireless communications, due to the large amount of available bandwidth that may be key to meet the rapidly increasing data rate requirements. Before deploying a system in this band, a detailed wireless channel analysis is required as the basis for proper design and testing of system implementations. One of the most important deployment scenarios of this band is the outdoor microcellular environment, where the Transmitter (Tx) and the Receiver (Rx) have a significant height difference (typically \$ \textbackslash ge 10\$ m). In this paper, we present double-directional (i.e., directionally resolved at both link ends) channel measurements in such a microcellular scenario encompassing street canyons and an open square. Measurements are done for a 1 GHz bandwidth between 145-146 GHz and an antenna beamwidth of 13 degree; distances between Tx and Rx are up to 85 m and the Tx is at a height of 11.5 m from the ground. The measurements are analyzed to estimate path loss, shadowing, delay spread, angular spread, and multipath component (MPC) power distribution. These results allow the development of more realistic and detailed THz channel models and system performance assessment.},
  pubstate = {preprint},
  keywords = {Electrical Engineering and Systems Science - Signal Processing},
  file = {E\:\\文献库\\Zotero\\storage\\A8C2ZCCQ\\Abbasi et al_2021_THz Band Channel Measurements and Statistical Modeling for Urban Microcellular.pdf;E\:\\文献库\\Zotero\\storage\\F2ISUF66\\2112.html}
}

