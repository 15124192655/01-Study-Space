  title = {Improvement of Adaptability to {{RSSI-based}} Positioning System Using Scaling Circle Method},
  booktitle = {2022 61st {{Annual Conference}} of the {{Society}} of {{Instrument}} and {{Control Engineers}} ({{SICE}})},
