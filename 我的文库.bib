@thesis{XiRongHeWiFiCSIFuDuHeXiangWeiChaiDeRenTiXingWeiGanZhiJiShuYanJiu2021,
  type = {硕士},
  title = {融合WiFi-CSI幅度和相位差的人体行为感知技术研究},
  author = {席, 建微},
  date = {2021},
  institution = {{福州大学}},
  doi = {10.27022/d.cnki.gfzhu.2021.001273},
  url = {https://kns.cnki.net/kcms2/article/abstract?v=3uoqIhG8C475KOm_zrgu4sq25HxUBNNTmIbFx6y0bOQ0cH_CuEtpsACWo5XIODBoIq_Hh48EPUXk2ThpJISLr_QkrsO_udGE&uniplatform=NZKPT},
  urldate = {2023-07-03},
  abstract = {随着智能家居的普及和发展,人体行为感知技术在智能家居应用需求的推动下得到了广泛的关注和发展,其中基于WiFi信号的人体行为感知技术由于部署方便、无需穿戴、不侵犯用户隐私且可实现穿墙检测等优点,得到了众多研究学者们的关注。室内人员的活动会影响周围WiFi信号的传播,通过分析WiFi的信道状态信息(Channel State Information,CSI),获取活动人员对WiFi信号的影响即可实现基于WiFi的人体行为感知。为满足智能家居中对人数检测和动作识别的应用需求,本文研究了融合WiFi-CSI幅值和相位差的人体行为感知技术。主要研究工作如下:首先,提出了融合WiFi-CSI信息的幅度和相位差的室内人体行为感知技术。为了模拟智能家居中应用环境,搭建了一个WiFi-CSI信息采集系统,分别设计了人在收发天线之间活动以及人在收发天线一侧活动两种采集方案,并通过在配置了Intel 5300网卡的计算机上安装的CSI Tool开源软件工具,实现了WiFi-CSI信息的采集。在此基础上,提取WiFi-CSI信息的幅度和接收天线对之间的相位差用于人体行为识别,并针对由于采集设备硬件及软件导致的CSI相位差异常问题,提出了一种新的相位差矫正方法,通过对CSI相位差进行分段中心化处理,有效解决了CSI相位差异常跳变问题。其次,提出了一种基于SVM的人数及动作识别方法。该方法利用滑动窗口方式提取WiFi-CSI幅值的时、频域特征和相位差的时域特征构成多维特征集,并采用支持向量机(Support Vector Machines,SVM)算法实现了同时对室内人数及动作的识别。实验结果表明,该方法在两种实验方案中对人数识别的平均准确率分别为93.5\%和91.5\%,动作识别的平均准确率分别为97.1\%和94.6\%,且具有较强的鲁棒性。最后,提出了一种基于深度学习的人数及动作识别方法。针对基于传统机器学习算法数据处理过程复杂、需要人为选取特征等问题,提出了利用深度学习网络进行人数及动作识别的方法。该方法通过将WiFi-CSI的幅值和相位差信息作为深度网络的输入,运用二维卷积神经网络搭建了一个人数及动作识别的多任务残差网络,同时针对上述多任务问题,设计了一种多任务自适应损失函数,有效地平衡了两子任务的学习能力,并实现了人数及动作的多任务感知。实验结果表明,该方法在人数识别方面的准确率为100\%,在动作识别方面的平均准确率为99.4\%。相较于文中采用的基于SVM的算法,识别效果有显著的提高。},
  langid = {chinese},
  keywords = {Convolutional neural network,Human action recognition,Human counting,Support Vector Machines,人数检测,信道状态信息,动作识别,卷积神经网络 Channel State Information,支持向量机},
  file = {E:\文献库\Zotero\storage\YETC56HT\席_2021_融合WiFi-CSI幅度和相位差的人体行为感知技术研究.pdf}
}

