}

@report{zhengCoverageAnalysisJoint2022,
  type = {preprint},
  title = {Coverage Analysis of Joint Localization and Communication in {{THz}} Systems with {{3D}} Arrays},
  author = {Zheng, Pinjun and Ballal, Tarig and Chen, Hui and Wymeersch, Henk and Al-Naffouri, Tareq Y.},
  date = {2022-10-27},
  doi = {10.36227/techrxiv.21385080},
  url = {https://www.techrxiv.org/articles/preprint/Coverage_Analysis_of_Joint_Localization_and_Communication_in_THz_Systems_with_3D_Arrays/21385080},
  urldate = {2023-09-19},
  abstract = {{$<$}p{$>$}As a key enabler of Terahertz (THz)-based wireless technologies, large-scale multiple-input-multiple-output systems are well known for their advantages in both communication and localization. Different from existing works that mostly focus on planar arrays, this paper first explores the potential of the three-dimensional (3D) spatial array structure in joint localization and communication coverage enhancement. We consider a THz-band wireless system where a user is equipped with a 3D array receiving downlink far-field signals from multiple base stations with known positions and orientations over Rician fading channels. First, we derive the constrained Cramér-Rao bound (CCRB) for the localization (i.e., position and orientation estimation) performance, based on which we define the localization coverage metrics. Then, we derive the communication key performance indicators (KPIs) including instantaneous signal-to-noise ratio, outage probability, and ergodic capacity, and define the corresponding coverage metrics. To facilitate localization applications using 3D arrays, a maximum likelihood-based algorithm for joint user equipment (UE) position and orientation estimation is proposed, which is initialized by a least squares-based solution. Our numerical results show the 3D array configuration offers overall higher coverage than the planar array w.r.t. both localization and communication KPIs, although with minor performance loss in certain UE positions and orientations. The proposed localization algorithm is also verified to be efficient in simulations as it attains the derived CCRB.{$<$}/p{$>$}},
  file = {E:\文献库\Zotero\storage\J8MBUZZV\Zheng et al_2022_Coverage analysis of joint localization and communication in THz systems with.pdf}
}

@thesis{ZhengXinYuYiDongTongXinWangLuoDingWeiFeiShiJuWuChaiJiLiYuBuChangJiShuYanJiu2022,
  type = {博士},
  title = {移动通信网络定位非视距误差机理与补偿技术研究},
  author = {{郑心雨}},
  date = {2022},
  institution = {{北京邮电大学}},
  doi = {10.26969/d.cnki.gbydu.2022.000253},
  url = {https://kns.cnki.net/kcms2/article/abstract?v=3uoqIhG8C447WN1SO36whFuPQ0yKi4pXSQlJ_W8wBD9JRPlAs_d8BxrIG2I-0luNbhKkBrAqX3Hh5T3bXmeNphvPKKRwoa_J&uniplatform=NZKPT},
  urldate = {2023-10-13},
  abstract = {导航与位置服务产业成为继互联网、移动通信之后发展最快的新型产业之一,在智慧城市、智慧交通、智慧物流、智慧医疗、智慧矿山、工业互联网等新兴智能领域发挥着举足轻重的作用。基于移动通信网络的定位技术是实现广域无缝高精度位置服务的重要手段,大带宽、高灵活性、密集组网等5G定位技术的特性使其成为未来广域无缝高精度位置服务最具潜力的技术之一。但在城市峡谷、室内、地下等复杂环境中的非视距现象给基于移动通信网络定位带来了多观测量非视距误差机理分析与建模难题、弱直达径传输条件下的非视距误差抑制难题、无直达径传输条件下非视距误差补偿难题。针对上述问题,本文开展了方法创新与工程实践,主要研究成果及创新点包括:1、针对移动通信网络定位多观测量非视距误差机理分析与建模难题,通过分析室内非视距条件下的信道功率时延谱发现了非视距条件下的直达径具有隐蔽现象,并根据该现象将非视距分为弱直达径非视距和无直达径非视距两类,分析了这两类非视距现象与无线信号传播的关联机理,提出了基于双级支持向量机的非视距类型智能识别方法;理论推导并分析了移动通信网络定位误差受观测量误差影响的关联关系,构建了基于非视距误差灵敏度的测量误差与定位误差关联模型;提出了基于生成对抗网络的非视距误差建模方法,通过引入实测误差样本进行对抗训练,实现了移动通信网络定位多观测量非视距误差机理分析与建模。实测结果表明,基于生成对抗网络的非视距误差建模方法的测距误差建模和测角误差建模准确度分别比基于路径搜索的高效射线追踪法提升12.56\%(1σ)和13.91\%(1σ)。2、针对无线信号弱直达径传输条件下的非视距误差抑制难题,本文分析了 5G下行定位参考信号、上行定位参考信号和信道状态信息参考信号的特点,建立了基于阵列天线空分复用的定位信号播发及接收策略,并分析了该策略下移动通信网络测距性能与通信性能,实现了定位信号与通信信号的同频空分复用;利用接收信号中信号特征值和噪声特征值的高区分度特性,提出了基于特征值的非监督多径数量估计方法,实现了无需先验信息的多径数量估计;提出了基于最强路径剥离的时延与角度非视距误差抑制方法,实现了弱直达径传输条件下的非视距误差抑制。仿真实验表明,本文提出的基于最强路径剥离的非视距误差抑制方法的测距精度和测角精度分别比基于信道冲击响应特征校正直达径的非视距抑制算法提升17.89\%(1σ)和16.78\%(1σ)。3、针对无直达径传输条件下非视距误差补偿难题,分析了现有非视距误差补偿方法因需要先验信息而可用性低的现状,提出了基于反演虚拟基站的非视距误差补偿方法:通过残差筛选的定位方法获取初始定位结果,利用行人航迹推算的短期高精度和连续定位特性,减少初始定位结果的非视距基站的引入;建立反演虚拟基站构建策略,基于多次初始位置对虚拟基站位置进行反向推算,实现了无需地图信息的虚拟基站构建;提出基于残差动态权重的融合定位算法,实现了无需先验信息的无直达径传输条件下非视距误差补偿,提升了定位精度。仿真实验表明,基于反演虚拟基站的无直达径非视距误差补偿方法在到达时间差定位、往返测距定位和到达角度定位三种定位方法的定位精度分别比基于已知反射体平面的虚拟基站方法的定位精度提升 14.96\%(1σ)、13.15\%(1σ)和 14.33\%(1σ)。4、设计并搭建了非视距误差抑制与补偿关键技术综合仿真平台与实验验证系统,将本文提出的基于生成对抗网络的非视距误差建模方法、基于最强路径剥离的弱直达径非视距误差抑制方法、基于反演虚拟基站的无直达径非视距误差补偿方法统一纳入到所设计的定位流程和架构内,对本文提出的理论方法创新进行了集成测试和实验验证,实验结果表明本文提出的方法能够相互兼容,集成后的定位精度较现有方法集成的定位精度提高了 22.34\%(1σ)。},
  langid = {chinese},
  keywords = {/unread,Error Mitigation and Compensation,Mobile Communication Network Positioning,Non-line-of-sight Error Mechanism,移动通信网络定位,误差抑制补偿,非视距误差机理},
  file = {E:\文献库\Zotero\storage\HQ4H4RYQ\郑_2022_移动通信网络定位非视距误差机理与补偿技术研究.pdf}
}

@thesis{ZhongBoWenCellFreeMassiveMIMOXiTongShangXingHeBingYuXiaXingYuBianMaSuanFaYanJiu2020,
  type = {硕士},
  title = {Cell-Free Massive MIMO系统上行合并与下行预编码算法研究},
  author = {{仲博文}},
  date = {2020},
  institution = {{哈尔滨工业大学}},
  doi = {10.27061/d.cnki.ghgdu.2020.003049},
  url = {https://kns.cnki.net/kcms2/article/abstract?v=3uoqIhG8C475KOm_zrgu4lQARvep2SAkyRJRH-nhEQBuKg4okgcHYh4j6ySGK3yLytL7MCbSDjhlbkZuQRi4wFJ3JzdvIgvi&uniplatform=NZKPT},
  urldate = {2023-08-01},
  abstract = {大规模天线系统作为5G核心技术之一在近些年得到了长足的发展,而作为分布式大规模天线系统的一个分支,Cell-Free Massive MIMO系统凭借其独特优势而逐渐成为未来移动通信网络的研究热点之一。其核心思想为使网络中大量分布式无线接入点通过相互协作,在相同的时频资源下为相对较少的用户提供服务,从而得以在避免越区切换的同时对抗阴影衰落并有效抑制干扰。然而这种分布式架构使得各无线接入点到用户间的信道不再满足独立同分布,传统集中式大规模天线系统中的很多上下行链路信号处理方法不再适用,因此上下行链路中信号处理是目前Cell-Free Massive MIMO系统中需要研究的问题之一。本文首先分析了Cell-Free Massive MIMO系统的基本架构,包括其信道模型与传输协议。论述了现有的上行链路接收合并算法与下行链路中的预编码算法,并给出了相应的上行与下行链路频谱效率表达式,分析了现有上行合并算法与下行预编码算法中存在的问题与不足。同时,在不同场景下将Cell-Free Massive MIMO系统与传统的Cellular Massive MIMO系统的上下行频谱效率性能进行了对比分析。随后介绍了Cell-Free Massive MIMO系统中前向链路受限的模型,并详细阐述了前向链路容量对系统上行与下行链路频谱效率表达式的影响。然后,针对现有的上行链路中合并算法与下行链路中预编码算法中存在的问题,本文考虑借助深度学习中的深度卷积网络来对上行合并算法与下行预编码算法进行改进。论文首先介绍了深度卷积神经网络的工作原理,以及无监督学习的思想,并以此为基础设计了一种全新的上行合并与下行预编码方案,并对该方案的频谱效率性能以及所需计算复杂度进行了分析。最后,针对传统Cell-Free Massive MIMO系统架构的可扩展性问题进行了讨论,并提出了一种以用户为中心的系统架构来满足系统可扩展条件,并推导出该架构中上下行频谱效率性能表达式。进一步针对该架构特点,设计出一种改进的上行合并与下行预编码算法,并分析了前向链路容量对其频谱效率性能的影响。结尾总结全文,并给出了Cell-Free Massive MIMO架构上行与下行信号处理问题中的一些待解决问题和研究方向。},
  langid = {chinese},
  keywords = {/unread,cell-free,Cell-Free,deep learning,Massive MIMO,precoding,receiver combining,大规模天线阵列,接收合并,深度学习,预编码},
  file = {E:\文献库\Zotero\storage\DWZ4IRDJ\仲博文-2020-Cell-Free Massive MIMO系统上行合并与下行预编码算法研究.pdf}
}

